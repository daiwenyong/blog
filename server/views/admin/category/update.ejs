<%- include('../layout.ejs')%>
<div class="container-fluid">
  <ol class="breadcrumb">
    <li><a href="/api/admin/category">分类首页</a></li>
    <li class="active">分类修改</li>
  </ol>
  <h3>分类修改</h3>

<!--action='/api/admin/category/update' action不写默认是本地地址带requery 如果写了地址就不会有requery-->
  <form method="post">
    <div class="form-group">
      <label>分类名称</label>
      <input type="text" class="form-control" name="category" value="<%= cate%>">
    </div>

    <button type="submit" class="btn btn-default">点击修改分类</button>
  </form>
</div>
